        android:theme="@style/AppTheme">
        <activity
            android:name="com.hgfgrfcv.utyhfgbrgf.ui.WelcomeActivity"
            android:icon="@mipmap/app_logo"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.hgfgrfcv.utyhfgbrgf.ui.WebViewActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.hgfgrfcv.utyhfgbrgf.ui.LoginActivity"
