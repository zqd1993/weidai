<?xml version="1.0" encoding="utf-8"?>
<com.nfhyrhd.nfhsues.fenqibeiyongjinw.ScrollYSwipeRefreshLayoutFenQiBeiYongJin xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipeRefreshLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f6f6f6"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="40dp"
                android:scaleType="centerCrop"
                android:src="@drawable/zdfhrtsu" />

            <androidx.cardview.widget.CardView
                android:layout_width="76dp"
                android:layout_height="76dp"
                app:cardBackgroundColor="@color/default_shadow_color"
                app:cardCornerRadius="5dp"
                app:cardElevation="0dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop" />
            </androidx.cardview.widget.CardView>


        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:id="@+id/main_top_img"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="40dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zdfhrtsu" />

                    <androidx.cardview.widget.CardView
                        android:layout_width="76dp"
                        android:layout_height="76dp"
                        app:cardBackgroundColor="@color/default_shadow_color"
                        app:cardCornerRadius="5dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="false"
                        app:cardUseCompatPadding="true">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:scaleType="centerCrop" />
                    </androidx.cardview.widget.CardView>


                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:src="@drawable/sy_bg" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="40dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zdfhrtsu" />

                    <androidx.cardview.widget.CardView
                        android:layout_width="76dp"
                        android:layout_height="76dp"
                        app:cardBackgroundColor="@color/default_shadow_color"
                        app:cardCornerRadius="5dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="false"
                        app:cardUseCompatPadding="true">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:scaleType="centerCrop" />
                    </androidx.cardview.widget.CardView>


                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="40dp"
                    android:text="@string/app_name"
                    android:textColor="@color/white"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:visibility="gone"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="40dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zdfhrtsu" />

                    <androidx.cardview.widget.CardView
                        android:layout_width="76dp"
                        android:layout_height="76dp"
                        app:cardBackgroundColor="@color/default_shadow_color"
                        app:cardCornerRadius="5dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="false"
                        app:cardUseCompatPadding="true">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:scaleType="centerCrop" />
                    </androidx.cardview.widget.CardView>


                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="80dp"
                    android:src="@drawable/kdtusrtyu"
                    android:visibility="gone"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginEnd="30dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:layout_marginTop="150dp"
                    android:background="@drawable/bg_corner_10_white">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp"
                        android:text="最高可申请额度（元）"
                        android:textColor="#999999"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/progress_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="15dp"
                        android:text="100000"
                        android:textColor="@color/text_normal_color"
                        android:textSize="33sp"
                        android:textStyle="bold" />

                    <SeekBar
                        android:id="@+id/pb_progressbar"
                        style="@style/mProgressBarStyle"
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="20dp"
                        android:duplicateParentState="true"
                        android:max="100"
                        android:progress="50"
                        android:progressDrawable="@drawable/main_pb_bg"
                        android:thumb="@null" />

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="35dp"
                        android:layout_marginEnd="35dp"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:text="2000"
                            android:textColor="#ff546a"
                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:text="200000"
                            android:textColor="#ff546a"
                            android:textSize="14sp" />

                    </FrameLayout>

                    <com.lihang.ShadowLayout
                        android:id="@+id/lijisq_sl"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="30dp"
                        app:hl_cornerRadius="50dp"
                        android:layout_marginStart="55dp"
                        android:layout_marginEnd="55dp"
                        android:layout_marginTop="30dp"
                        app:hl_layoutBackground="#ff546a">

                        <TextView
                            android:id="@+id/shenqing_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textColor="@color/white"
                            android:text="立即申请"
                            android:gravity="center"
                            android:paddingBottom="10dp"
                            android:paddingTop="10dp"/>

                    </com.lihang.ShadowLayout>

                </LinearLayout>

            </FrameLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/zdfhrtsu" />

                <androidx.cardview.widget.CardView
                    android:layout_width="76dp"
                    android:layout_height="76dp"
                    app:cardBackgroundColor="@color/default_shadow_color"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp"
                    app:cardPreventCornerOverlap="false"
                    app:cardUseCompatPadding="true">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <TextView
                android:id="@+id/jx_bg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorPrimary"
                android:gravity="center"
                android:paddingTop="30dp"
                android:paddingBottom="30dp"
                android:text="列表"
                android:textColor="@color/white"
                android:textSize="18sp"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/zdfhrtsu" />

                <androidx.cardview.widget.CardView
                    android:layout_width="76dp"
                    android:layout_height="76dp"
                    app:cardBackgroundColor="@color/default_shadow_color"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp"
                    app:cardPreventCornerOverlap="false"
                    app:cardUseCompatPadding="true">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>
        </FrameLayout>

        <LinearLayout
            android:id="@+id/flipper_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/white"
            android:gravity="center_vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:text="消息快讯"
                android:textStyle="bold"
                android:textColor="#ff546a"
                android:textSize="@dimen/textSize_16" />

            <ViewFlipper
                android:id="@+id/view_flipper"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:layout_weight="1" />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/tuijian_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="10dp">

            <View
                android:layout_width="5dp"
                android:layout_height="20dp"
                android:layout_marginStart="20dp"
                android:background="#ff546a" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/zdfhrtsu" />

                <androidx.cardview.widget.CardView
                    android:layout_width="76dp"
                    android:layout_height="76dp"
                    app:cardBackgroundColor="@color/default_shadow_color"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp"
                    app:cardPreventCornerOverlap="false"
                    app:cardUseCompatPadding="true">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:text="优质产品"
                android:textColor="#ff546a"
                android:textSize="@dimen/textSize_16"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/zdfhrtsu" />

                <androidx.cardview.widget.CardView
                    android:layout_width="76dp"
                    android:layout_height="76dp"
                    app:cardBackgroundColor="@color/default_shadow_color"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp"
                    app:cardPreventCornerOverlap="false"
                    app:cardUseCompatPadding="true">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/click_fl"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/goods_list_ll"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone" />

            </androidx.core.widget.NestedScrollView>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/zdfhrtsu" />

                <androidx.cardview.widget.CardView
                    android:layout_width="76dp"
                    android:layout_height="76dp"
                    app:cardBackgroundColor="@color/default_shadow_color"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp"
                    app:cardPreventCornerOverlap="false"
                    app:cardUseCompatPadding="true">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <com.youth.banner.Banner
                android:id="@+id/goods_banner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/zdfhrtsu" />

                <androidx.cardview.widget.CardView
                    android:layout_width="76dp"
                    android:layout_height="76dp"
                    app:cardBackgroundColor="@color/default_shadow_color"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp"
                    app:cardPreventCornerOverlap="false"
                    app:cardUseCompatPadding="true">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <TextView
                android:id="@+id/no_data_tv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:background="@color/color_efefef"
                android:gravity="center"
                android:text="暂无数据"
                android:textColor="@color/text_normal_color"
                android:textSize="18sp"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:padding="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/zdfhrtsu" />

                <androidx.cardview.widget.CardView
                    android:layout_width="76dp"
                    android:layout_height="76dp"
                    app:cardBackgroundColor="@color/default_shadow_color"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp"
                    app:cardPreventCornerOverlap="false"
                    app:cardUseCompatPadding="true">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>
        </FrameLayout>

    </LinearLayout>

</com.nfhyrhd.nfhsues.fenqibeiyongjinw.ScrollYSwipeRefreshLayoutFenQiBeiYongJin>